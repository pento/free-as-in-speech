{
	"name": "Free (as in Speech)",
	"version": "0.0.0",
	"description": "An awesome new browser extension",
	"homepage_url": "https://github.com/pento/free-as-in-speech",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"applications": {
		"gecko": {
			"id": "free-as-in-speech@pento.github.io",
			"strict_min_version": "67.0"
		}
	},
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"pageAction",
		"storage",
		"https://www.wix.com/*"
	],
	"page_action": {
		"default_icon": "icon.png",
		"show_matches": [ "https://www.wix.com/dashboard/*" ]
	},
	"options_ui": {
		"chrome_style": true,
		"page": "options.html"
	},
	"background": {
		"persistent": false,
		"scripts": [
			"build/browser-polyfill.min.js",
			"build/background.js"
		]
	}
}
